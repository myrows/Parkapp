<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MiAparcamientoActivity">

    <Button
        android:id="@+id/buttonDesocupar"
        android:layout_width="408dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:layout_marginBottom="184dp"
        android:background="@color/colorPrimaryDark"
        android:text="DESOCUPAR"
        android:textColor="@color/cardview_light_background"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardView4"
        app:layout_constraintVertical_bias="0.766" />

    <TextView
        android:id="@+id/zonaMiAparcamiento"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="36dp"
        android:layout_marginRight="36dp"
        android:text="TextView"
        android:textColor="@color/colorPrimary"
        app:layout_constraintEnd_toEndOf="@+id/imagenMiAparcamiento"
        app:layout_constraintTop_toBottomOf="@+id/imagenMiAparcamiento" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView3"
        android:layout_width="218dp"
        android:layout_height="53dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imagenMiAparcamiento"
        app:layout_constraintStart_toStartOf="@+id/imagenMiAparcamiento"
        app:layout_constraintTop_toBottomOf="@+id/imagenMiAparcamiento"
        app:layout_constraintVertical_bias="0.131">

        <TextView
            android:id="@+id/NombreMiAparcamiento"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="TextView" />
    </androidx.cardview.widget.CardView>

    <ImageView
        android:id="@+id/imagenMiAparcamiento"
        android:layout_width="0dp"
        android:layout_height="183dp"
        android:layout_marginTop="44dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_parkapp" />

    <Button
        android:id="@+id/buttonHistorial"
        android:layout_width="91dp"
        android:layout_height="81dp"
        android:background="@drawable/ic_access_time_black_24dp"
        app:layout_constraintBottom_toTopOf="@+id/buttonDesocupar"
        app:layout_constraintEnd_toEndOf="@+id/cardView3"
        app:layout_constraintStart_toStartOf="@+id/cardView3"
        app:layout_constraintTop_toBottomOf="@+id/cardView3" />

</androidx.constraintlayout.widget.ConstraintLayout>